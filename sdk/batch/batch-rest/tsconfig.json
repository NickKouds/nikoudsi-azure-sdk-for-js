{
  "compilerOptions": {
    "module": "es6",
    "target": "es2015",
    "moduleResolution": "node",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist-esm",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@azure/batch": [
        "./src/generated/index.ts"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "./test/**/*.ts",
    "samples-dev/**/*.ts"
  ],
  "exclude": [
    "src/utilities/utils.ts"
  ]
}
